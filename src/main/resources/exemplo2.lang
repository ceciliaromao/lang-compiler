abstract data Racional {
    numerador :: Int;
    denominador :: Int;

    soma(a :: Racional, b :: Racional) : Racional {
        r = new Racional;
        r.numerador = a.numerador * b.denominador + b.numerador * a.denominador;
        r.denominador = a.denominador * b.denominador;
        return r;
    }
}

